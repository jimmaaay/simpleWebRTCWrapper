{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": [
      "webrtc"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}